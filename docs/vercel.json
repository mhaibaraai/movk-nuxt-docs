{
  "rewrites": [
    {
      "source": "/llms.txt",
      "destination": "/_llms.txt"
    },
    {
      "source": "/llms-full.txt",
      "destination": "/_llms-full.txt"
    }
  ],
  "headers": [
    {
      "source": "/_llms(.*).txt",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, s-maxage=3600"
        }
      ]
    }
  ]
}
