{
  "name": "nuxt-docs-workspace",
  "type": "module",
  "version": "1.11.0",
  "private": true,
  "packageManager": "pnpm@10.28.2",
  "license": "MIT",
  "homepage": "https://docs.mhaibaraai.cn",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mhaibaraai/movk-nuxt-docs.git"
  },
  "sideEffects": false,
  "scripts": {
    "postinstall": "pnpm dev:prepare",
    "build": "NODE_OPTIONS='--max-old-space-size=8192' nuxt build docs --extends ../layer",
    "dev": "nuxt dev docs --extends ../layer",
    "dev:prepare": "nuxt prepare layer && nuxt prepare docs --extends ../layer",
    "release": "pnpm release:layer && release-it",
    "release:layer": "release-it --config .release-it.layer.json && cd layer && npm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "nuxt typecheck layer",
    "clean": "movk-clean"
  },
  "devDependencies": {
    "@movk/core": "^1.1.0",
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/eslint": "^1.13.0",
    "@nuxt/eslint-config": "^1.13.0",
    "@release-it/bumper": "^7.0.5",
    "@release-it/conventional-changelog": "^10.0.4",
    "eslint": "^9.39.2",
    "nuxt": "^4.3.0",
    "release-it": "^19.2.4",
    "typescript": "^5.9.3",
    "vue-tsc": "^3.2.4"
  }
}
