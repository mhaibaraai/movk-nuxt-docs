{
  "name": "nuxt-docs-workspace",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.18.3",
  "license": "MIT",
  "homepage": "https://docs.mhaibaraai.cn",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mhaibaraai/movk-nuxt-docs.git"
  },
  "sideEffects": false,
  "scripts": {
    "postinstall": "nr dev:prepare",
    "build": "nuxt build docs --extends ../layer",
    "dev": "nuxt dev docs --extends ../layer",
    "dev:prepare": "nuxt prepare layer && nuxt prepare docs --extends ../layer",
    "release": "nr release:layer && release-it",
    "release:layer": "release-it --config .release-it.layer.json && cd layer && npm publish --access public",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "nuxt typecheck layer",
    "up": "taze major -wIr",
    "clean": "tsx scripts/rm.ts"
  },
  "devDependencies": {
    "@antfu/ni": "^26.2.0",
    "@nuxt/devtools": "^2.6.5",
    "@nuxt/eslint": "^1.9.0",
    "@release-it/bumper": "^7.0.5",
    "@release-it/conventional-changelog": "^10.0.1",
    "eslint": "^9.38.0",
    "fast-glob": "^3.3.3",
    "nuxt": "^4.1.3",
    "release-it": "^19.0.5",
    "taze": "^19.8.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vue-tsc": "^3.1.1"
  }
}
